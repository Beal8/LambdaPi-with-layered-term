(lang dune 2.9)
(name sedlex)
(version 2.4)
(library
 (name sedlex)
 (kind normal)
 (archives (byte sedlex.cma) (native sedlex.cmxa))
 (plugins (byte sedlex.cma) (native sedlex.cmxs))
 (native_archives sedlex.a)
 (requires gen uchar)
 (modes byte native)
 (modules
  (singleton
   (name Sedlexing)
   (obj_name sedlexing)
   (visibility public)
   (impl)
   (intf))))
(library
 (name sedlex.ppx)
 (kind ppx_rewriter)
 (archives (byte ppx/sedlex_ppx.cma) (native ppx/sedlex_ppx.cmxa))
 (plugins (byte ppx/sedlex_ppx.cma) (native ppx/sedlex_ppx.cmxs))
 (native_archives ppx/sedlex_ppx.a)
 (requires ppxlib.ast ppxlib sedlex)
 (ppx_runtime_deps sedlex)
 (main_module_name Sedlex_ppx)
 (modes byte native)
 (modules
  (wrapped
   (main_module_name Sedlex_ppx)
   (modules
    ((name Ppx_sedlex)
     (obj_name sedlex_ppx__Ppx_sedlex)
     (visibility public)
     (impl))
    ((name Sedlex)
     (obj_name sedlex_ppx__Sedlex)
     (visibility public)
     (impl)
     (intf))
    ((name Sedlex_cset)
     (obj_name sedlex_ppx__Sedlex_cset)
     (visibility public)
     (impl)
     (intf))
    ((name Unicode)
     (obj_name sedlex_ppx__Unicode)
     (visibility public)
     (impl)
     (intf)))
   (alias_module
    (name Sedlex_ppx)
    (obj_name sedlex_ppx)
    (visibility public)
    (kind alias)
    (impl))
   (wrapped true))))
